  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch local",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main.dart",
        "--dart-define",
        "WENV=local"
      ]
    },
    {
      "name": "Launch local_test",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main.dart",
        "--dart-define",
        "WENV=local_test"
      ]
    },
    {
      "name": "Launch prod",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "production",
        "--target",
        "lib/main.dart",
        "--dart-define",
        "WENV=prod"
      ]
    }
  ]
}
